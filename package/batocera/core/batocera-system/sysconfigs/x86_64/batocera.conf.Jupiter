# Steam Deck LCD default values

# resolution
es.resolution=800x1280.60.00

# default card
audio.device=alsa_output.pci-0000_04_00.5-platform-acp5x_mach.0.pro-output-1

# xorg rotation
display.rotate.eDP-1=1

# drm rotation, for the splash screen
display.rotate.EDP=1

# default suspend on handled boards
system.suspendmode=suspend
