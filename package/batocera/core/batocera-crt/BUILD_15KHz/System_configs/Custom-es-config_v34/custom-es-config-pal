#!/bin/bash
##-Default Resolution-##
xrandr -display :0.0 --newmode "640x480" 12.296875 640 658 716 787 480 534 539 625 interlace  -hsync -vsync
xrandr -display :0.0 --addmode  [card_display] "640x480"
[640x480]xrandr -display :0.0 --output  [card_display] --mode "640x480"
    
