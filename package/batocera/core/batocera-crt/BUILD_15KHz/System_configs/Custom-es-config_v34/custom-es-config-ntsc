#!/bin/bash
##-Default Resolution-##
xrandr -display :0.0 --newmode "640x480" 12.146841 640 658 715 772 480 487 493 525 interlace  -hsync -vsync
xrandr -display :0.0 --addmode  [card_display] "640x480"
[640x480]xrandr -display :0.0 --output  [card_display] --mode "640x480"
    
xrandr -display :0.0 --newmode "720x480" 13.657329 720 740 804 868 480 487 493 525 interlace  -hsync -vsync
