#!/bin/bash
##-Default Resolution-##
xrandr -display :0.0 --newmode "640x480" 13.038390 640 666 727 831 480 483 489 523 interlace  -hsync -vsync
xrandr -display :0.0 --addmode  [card_display] "640x480"
[640x480]xrandr -display :0.0 --output  [card_display] --mode "640x480"

