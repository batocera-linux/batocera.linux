From cdf3d1a5f4f1465c45775ac6948cb11d07e09043 Mon Sep 17 00:00:00 2001
From: Ntemis <ierokipides.dem@gmail.com>
Date: Sun, 21 Jun 2020 01:48:10 +0300
Subject: [PATCH] RK3288: Raise ethernet speed

---
 arch/arm/boot/dts/rk3288-miniarm.dts | 2 +-
 arch/arm/boot/dts/rk3288-miqi.dts    | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm/boot/dts/rk3288-miniarm.dts b/arch/arm/boot/dts/rk3288-miniarm.dts
index 7f6ed1ff6..af37c2c20 100644
--- a/arch/arm/boot/dts/rk3288-miniarm.dts
+++ b/arch/arm/boot/dts/rk3288-miniarm.dts
@@ -373,7 +373,7 @@
 	pinctrl-names = "default";
 	pinctrl-0 = <&rgmii_pins>;
 	tx_delay = <0x30>;
-	rx_delay = <0x10>;
+	rx_delay = <0x20>;
 	status = "okay";
 };
 
diff --git a/arch/arm/boot/dts/rk3288-miqi.dts b/arch/arm/boot/dts/rk3288-miqi.dts
index b3cb67793..e51e4e1cc 100644
--- a/arch/arm/boot/dts/rk3288-miqi.dts
+++ b/arch/arm/boot/dts/rk3288-miqi.dts
@@ -337,7 +337,7 @@
 	pinctrl-names = "default";
 	pinctrl-0 = <&rgmii_pins>;
 	tx_delay = <0x30>;
-	rx_delay = <0x10>;
+	rx_delay = <0x20>;
 	status = "okay";
 };
 
