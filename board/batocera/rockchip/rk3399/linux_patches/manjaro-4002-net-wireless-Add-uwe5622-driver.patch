+		return ret;
+	}
+	WCN_DEBUG("read AD_DCXO_BONDING_OPT val:0x%x\n", temp_val);
+	if ((temp_val & tsx_mode) == tsx_mode)
+		WCN_INFO("clock mode: TSX\n");
+	else {
+		WCN_INFO("clock mode: TCXO, outside clock\n");
+		//marlin_avdd18_dcxo_enable(false);
+	}
+
+	return ret;
+}
