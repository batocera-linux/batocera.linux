+int sprdwl_send_cmd(struct sprdwl_priv *priv, struct sprdwl_msg_buf *msg)
+{
+	int ret;
+	struct sk_buff *skb;
+
+	skb = msg->skb;
+	ret = sprdwl_intf_tx(priv, msg);
+	if (ret) {
+		wl_err("%s TX cmd Err: %d\n", __func__, ret);
+		/* now cmd msg droped */
+#if !defined(UWE5621_FTR)
+		dev_kfree_skb(skb);
+#endif
+	}
+
+	return ret;
+}
