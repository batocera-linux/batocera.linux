+++ linux-6.5.8/drivers/net/wireless/uwe5622/unisocwifi/tcp_ack.c	2023-10-23 13:14:40.263334166 +0200
@@ -0,0 +1,606 @@
+#include <uapi/linux/if_ether.h>
+#include <uapi/linux/tcp.h>
+#include <uapi/linux/ip.h>
+#include <uapi/linux/in.h>
