diff --speed-large-files --no-dereference --minimal -Naur linux-6.5.8/drivers/net/wireless/uwe5622/unisocwcn/pcie/pcie.c linux-6.5.8/drivers/net/wireless/uwe5622/unisocwcn/pcie/pcie.c
