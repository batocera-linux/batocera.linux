diff --speed-large-files --no-dereference --minimal -Naur linux-6.5.8/drivers/net/wireless/uwe5622/unisocwcn/platform/wcn_log.c linux-6.5.8/drivers/net/wireless/uwe5622/unisocwcn/platform/wcn_log.c
