+				WCN_ERR("%s bind verify error:%d\n",
+					__func__, ret);
+				return ret;
+			}
+		}
+#endif
+		else
+			msleep(20);
+		if (i > 10)
+			return -1;
+	} while (i);
+
+	return 0;
+}
