+bool mdbg_rx_count_change(void)
+{
+	rx_count = sprdwcn_bus_get_rx_total_cnt();
+
+	WCN_INFO("rx_count:0x%llx rx_count_last:0x%llx\n",
+		rx_count, rx_count_last);
+
+	if ((rx_count == 0) && (rx_count_last == 0)) {
+		return true;
