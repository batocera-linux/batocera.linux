+		return -ENOMEM;
+	p = (struct sprdwl_cmd_11v *)msg->data;
+
+	p->cmd = SPRDWL_SUBCMD_SET;
+	p->value = (val << 16) | val;
+	/* len  only 8 =  cmd(2) + len(2) +value(4)*/
+	p->len = 8;
