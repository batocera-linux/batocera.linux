+		vif->priv->sched_scan_vif = NULL;
+		spin_unlock_bh(&priv->sched_scan_lock);
+	} else {
+		wl_ndev_log(L_ERR, ndev, "%s  scan stop failed\n", __func__);
+	}
+	return ret;
+}
