+static void wcn_usb_rx_trash_callback(struct wcn_usb_packet *packet)
+{
+	struct wcn_usb_rx_apostle *apostle;
+
+	apostle = wcn_usb_packet_get_pdata(packet);
+	wcn_usb_rx_apostle_free(apostle);
+}
