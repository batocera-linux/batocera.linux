+static int write_file(struct file *fp, char *buf, size_t len)
+{
+	loff_t offset = 0;
+
+	offset = vfs_llseek(fp, 0, SEEK_END);
+	if (kernel_write(fp, buf, len, offset) < 0) {
+		pr_err("kernel_write() for fp failed:");
+		return -1;
+	}
+
+	return 0;
+}
