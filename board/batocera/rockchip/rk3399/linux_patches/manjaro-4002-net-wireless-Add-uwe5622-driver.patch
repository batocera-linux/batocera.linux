+static int mdbg_snap_shoot_seq_show(struct seq_file *m, void *p)
+{
+	u8 *pdata;
+	u32 loop;
+
+	if (p) {
+		for (loop = 0; loop < 2; loop++) {
+			pdata = p + 8*loop;
+			seq_printf(m, "0x%02x%02x%02x%02x 0x%02x%02x%02x%02x ",
+					pdata[3], pdata[2], pdata[1], pdata[0],
+					pdata[7], pdata[6], pdata[5], pdata[4]);
+		}
+		seq_puts(m, "\n");
+	}
+
+	return 0;
+}
