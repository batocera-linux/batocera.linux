+static const u8 *wpa_scan_get_ie(u8 *res, u8 ie_len, u8 ie)
+{
+	const u8 *end, *pos;
+
+	pos = res;
+	end = pos + ie_len;
+	while (pos + 1 < end) {
+		if (pos + 2 + pos[1] > end)
+			break;
+		if (pos[0] == ie)
+			return pos;
+		pos += 2 + pos[1];
+	}
+	return NULL;
