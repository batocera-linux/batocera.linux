+		 struct net_device *dev, struct cfg80211_chan_def *chandef,
+		  u32 cac_time_ms);
+int sprdwl_cfg80211_channel_switch(struct wiphy *wiphy, struct net_device *dev,
+		struct cfg80211_csa_settings *params);
