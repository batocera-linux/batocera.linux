+int sprdwl_xmit_data2cmd_wq(struct sk_buff *skb, struct net_device *ndev)
+{
+#define FLAG_SIZE  5
+	struct sprdwl_vif *vif = netdev_priv(ndev);
+	u8 *temp_flag = "01234";
