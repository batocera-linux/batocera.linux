+static int find_type(char key)
+{
+	if ((key >= 'a' && key <= 'w') ||
+		(key >= 'y' && key <= 'z') ||
+		(key >= 'A' && key <= 'W') ||
+		(key >= 'Y' && key <= 'Z') ||
+		('_' == key))
+		return 1;
+	if ((key >= '0' && key <= '9') ||
+		('-' == key))
+		return 2;
+	if (('x' == key) ||
+		('X' == key) ||
+		('.' == key))
+		return 3;
+	if ((key == '\0') ||
+		('\r' == key) ||
+		('\n' == key) ||
+		('#' == key))
+		return 4;
+	return 0;
+}
