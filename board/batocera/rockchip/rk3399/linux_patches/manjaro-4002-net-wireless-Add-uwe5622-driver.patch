+		PCIE_INFO("%s {0x%lx,0x%lx} -- {0x%lx,0x%lx}\n",
+			  __func__, mpool_dm.vir, mpool_dm.phy,
+			  mpool_dm.vir + 0x20000,
+			  mpool_dm.phy + 0x20000);
+	}
+	if (len <= 0)
+		return NULL;
+	p = mpool_buffer;
+	memset(p, 0x56, len);
+	mpool_buffer += len;
