+static int handle_rsp_status_err(u8 cmd_id, s8 status)
+{
+	int flag = 0;
+
+	switch (cmd_id) {
+	case WIFI_CMD_DOWNLOAD_INI:
+		 if ((SPRDWL_CMD_STATUS_CRC_ERROR == status) ||
+			(SPRDWL_CMD_STATUS_INI_INDEX_ERROR == status) ||
+			(SPRDWL_CMD_STATUS_LENGTH_ERROR == status))
+			flag = -1;
+		 break;
+	default:
+		 flag = 0;
+		 break;
+	}
+
+	return flag;
+}
