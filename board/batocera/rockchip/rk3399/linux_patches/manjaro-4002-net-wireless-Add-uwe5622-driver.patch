+		skb = skb_realloc_headroom(skb, (DSCR_LEN + intf->hif_offset +
+						 MSG_PTR_LEN));
+		kfree_skb(temp_skb);
+		if (skb == NULL) {
+			wl_err("%s:%d failed to realloc skbuff: NULL\n",
+				   __func__, __LINE__);
