+static inline bool seqno_leq(unsigned short seq1, unsigned short seq2)
+{
+	bool ret = false;
+
+	if (((seq1 <= seq2) && ((seq2 - seq1) < MAX_SEQNO_BY_TWO)) ||
+	    ((seq1 > seq2) && ((seq1 - seq2) >= MAX_SEQNO_BY_TWO)))
+			ret = true;
+	return ret;
