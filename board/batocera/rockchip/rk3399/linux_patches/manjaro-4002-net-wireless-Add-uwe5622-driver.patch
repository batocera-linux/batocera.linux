diff --speed-large-files --no-dereference --minimal -Naur linux-6.5.8/drivers/net/wireless/uwe5622/unisocwcn/sdio/sdiohal_rx.c linux-6.5.8/drivers/net/wireless/uwe5622/unisocwcn/sdio/sdiohal_rx.c
