+	struct	mutex power_lock;
+	u32	power_state;
+	struct regulator *vddwifipa;
+	struct mutex vddwifipa_lock;
+	char	*write_buffer;
+	struct	delayed_work power_wq;
+	struct	work_struct load_wq;
+	struct	delayed_work cali_wq;
+	struct	completion download_done;
+};
