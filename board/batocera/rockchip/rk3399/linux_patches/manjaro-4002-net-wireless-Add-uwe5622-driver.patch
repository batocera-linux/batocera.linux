+int sprdwl_check_p2p_coex(struct sprdwl_priv *priv)
+{
+	if (mode_to_vif(priv, SPRDWL_MODE_P2P_CLIENT) ||
+		mode_to_vif(priv, SPRDWL_MODE_P2P_GO))
+		return -1;
+
+	return 0;
+}
