+static void mtty_tty_driver_exit(struct mtty_device *device)
+{
+	struct tty_driver *driver = device->driver;
+
+	tty_unregister_driver(driver);
+	tty_driver_kref_put(driver);
+	tty_port_destroy(device->port);
