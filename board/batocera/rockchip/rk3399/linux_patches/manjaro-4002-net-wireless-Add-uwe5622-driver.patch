+	for (i = 0; i < 3; i++) {
+		reg_val = a[i][1];
+		ret = sprdwcn_bus_reg_write(a[i][0], &reg_val, 4);
+		if (ret < 0) {
+			WCN_ERR("%s  error:%d\n", __func__, ret);
+			return;
+		}
+	}
+}
