+
+	for (i = 0; i < TP_TX_BUF_CNT; i++) {
+		tp_tx_buf[i] = kzalloc(TP_TX_BUF_LEN + PUB_HEAD_RSV,
+				       GFP_KERNEL);
+		if (!tp_tx_buf[i]) {
+			for (j = 0; j < i; j++) {
+				kfree(tp_tx_buf[j]);
+				tp_tx_buf[j] = NULL;
+			}
+			return -ENOMEM;
+		}
+	}
+	return 0;
+}
