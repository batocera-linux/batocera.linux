+static void wifi_reset_init(void)
+{
+	INIT_WORK(&wifi_rst_begin, wifi_reset_wq);
+	INIT_WORK(&wifi_rst_down, wifi_resume_wq);
+	init_completion(&wifi_reset_ready);
+	return;
+}
