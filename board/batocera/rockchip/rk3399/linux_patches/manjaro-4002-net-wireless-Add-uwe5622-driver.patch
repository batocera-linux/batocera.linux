+		return -ENODEV;
+	}
+	inout = ops->inout;
+
+	channel_debug_mbuf_from_user(chn, num);
+	/* if this is a rx channel */
+	if (inout == 0) { /* rx */
+		/* free each buf */
+		/* free mbuf list */
+		return wcn_usb_push_list_rx(chn, head, tail, num);
+	}
+	/* if this is a tx channel */
+	/* submit this mbuf list */
+	return wcn_usb_push_list_tx(chn, head, tail, num);
