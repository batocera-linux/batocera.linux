+
+		if (((subsys != MARLIN_BLUETOOTH) && (subsys != MARLIN_WIFI)) &&
+		    ((marlin_dev->power_state & 0x5) == 0)) {
+			wifipa_enable(0);
+			pmic_bound_xtl_assert(0);
+		}
+
+	} else {
+		if (((subsys == MARLIN_BLUETOOTH) &&
+		     ((marlin_dev->power_state & 0x4) == 0)) ||
+		    ((subsys == MARLIN_WIFI) &&
+		     ((marlin_dev->power_state & 0x1) == 0))) {
+			wifipa_enable(0);
+			pmic_bound_xtl_assert(0);
+		}
+	}
