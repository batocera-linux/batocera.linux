+	dscr->tx_ctrl.sw_rate = (type == SPRDWL_TYPE_DATA ? 0 : 1);
+	dscr->tx_ctrl.wds = 0;
+	dscr->sta_lut_index = lut_index;
+	dscr->tcp_udp_header_offset = 34;
