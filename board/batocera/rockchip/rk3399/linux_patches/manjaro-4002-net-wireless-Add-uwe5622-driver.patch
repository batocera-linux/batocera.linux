+void set_wifipa_status(int subsys, int val)
+{
+	return;
+
+	if (val == 1) {
+		if (((subsys == MARLIN_BLUETOOTH) || (subsys == MARLIN_WIFI)) &&
+		    ((marlin_dev->power_state & 0x5) == 0)) {
+			wifipa_enable(1);
+			pmic_bound_xtl_assert(1);
