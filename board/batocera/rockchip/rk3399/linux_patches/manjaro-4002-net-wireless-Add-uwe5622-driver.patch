+static void wcn_usb_callback_copy_tx(struct wcn_usb_packet *packet)
+{
+	struct wcn_usb_rx_tx *rx_tx;
+
+	rx_tx = wcn_usb_packet_get_pdata(packet);
+	wcn_usb_put_big_men(wcn_usb_packet_pop_buf(packet), rx_tx->channel);
