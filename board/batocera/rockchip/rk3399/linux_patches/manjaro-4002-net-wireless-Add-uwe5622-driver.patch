+					&gnss_efuse_data[0],
+					GNSS_EFUSE_DATA_SIZE);
+		ret = 0;
+		GNSSCOMM_ERR("%s 0x%x\n", __func__, gnss_efuse_data[0]);
+	} else
+		GNSSCOMM_INFO("%s no need back again\n", __func__);
