+uint16_t CRC16(uint8_t *buf, uint16_t len)
+{
+	uint16_t CRC = 0xFFFF;
+	uint16_t i;
+	uint8_t ch_char;
+
+	for (i = 0; i < len; i++) {
+		ch_char = *buf++;
+		CRC = CRC_table[(ch_char ^ CRC) & 15] ^ (CRC >> 4);
+		CRC = CRC_table[((ch_char >> 4) ^ CRC) & 15] ^ (CRC >> 4);
+	}
+	return CRC;
+}
