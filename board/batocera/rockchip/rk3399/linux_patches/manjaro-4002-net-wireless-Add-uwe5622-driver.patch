+	times_count++;
+	if (!(times_count % PERFORMANCE_COUNT)) {
+		sdiohal_pr_perf("tx avg time:%ld\n",
+			(time_total_ns / PERFORMANCE_COUNT));
+		time_total_ns = 0;
+		times_count = 0;
+	}
+
+	return ret;
