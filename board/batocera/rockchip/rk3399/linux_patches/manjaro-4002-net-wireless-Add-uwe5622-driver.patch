+int sdiohal_tx_set_eof(struct sdiohal_sendbuf_t *send_buf,
+	unsigned char *eof_buf)
+{
+	if ((!send_buf) || (!eof_buf))
+		return -EINVAL;
+
+	memcpy((void *)(send_buf->buf + send_buf->used_len),
+		   (void *)eof_buf, sizeof(struct sdio_puh_t));
+	send_buf->used_len += sizeof(struct sdio_puh_t);
+
+	return 0;
+}
