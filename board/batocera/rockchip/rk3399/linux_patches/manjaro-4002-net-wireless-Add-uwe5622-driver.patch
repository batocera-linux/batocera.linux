+
+#define GNSS_BOOTSTATUS_ADDRESS_M3         0x40aaff6c
+#define GNSS_BOOTSTATUS_ADDRESS_M3L        0x40aabf6c
+#define GNSS_BOOTSTATUS_ADDRESS SET_REG_VALUE(GNSS_BOOTSTATUS_ADDRESS_M3, \
+									GNSS_BOOTSTATUS_ADDRESS_M3L, 0)
+
+#endif
+
