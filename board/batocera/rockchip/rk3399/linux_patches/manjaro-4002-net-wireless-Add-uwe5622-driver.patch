+static int sprdwl_open(struct net_device *ndev)
+{
+	wl_ndev_log(L_DBG, ndev, "%s\n", __func__);
+#ifdef DFS_MASTER
+	netif_carrier_off(ndev);
+#endif
+
+	netif_start_queue(ndev);
+
+	return 0;
+}
