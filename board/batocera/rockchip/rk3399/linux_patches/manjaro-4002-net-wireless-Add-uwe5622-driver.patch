+static void sdiohal_debug_en(bool enable)
+{
+	unsigned char reg_val = 0;
+
+	/* ctl en */
+	sdiohal_aon_readb(WCN_DEBUG_CTL_REG, &reg_val);
+	if (enable)
+		reg_val |= WCN_CTL_EN;
