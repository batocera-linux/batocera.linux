+int get_list_num(struct list_head *list)
+{
+	int num = 0;
+	struct list_head *pos;
+	struct list_head *n_list;
+
+	if (list_empty(list))
+		return 0;
+	list_for_each_safe(pos, n_list, list)
+		num++;
+	return num;
+}
