+		return -ENOMEM;
+
+	ret = sscanf(buf+3, "%d %d %d %d",
+		     &test_cmd->channel,
+		     &test_cmd->command,
+		     &test_cmd->mbuf_num,
+		     &test_cmd->mbuf_len);
+	if (ret < 0) {
+		ret = -EINVAL;
