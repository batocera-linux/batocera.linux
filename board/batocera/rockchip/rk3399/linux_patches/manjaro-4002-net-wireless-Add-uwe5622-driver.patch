+static inline void
+sprdwl_rx_mh_data_process(struct sprdwl_rx_if *rx_if, void *data,
+			  int len, int buffer_type)
+{
+	mm_mh_data_process(&rx_if->mm_entry, data, len, buffer_type);
+}
