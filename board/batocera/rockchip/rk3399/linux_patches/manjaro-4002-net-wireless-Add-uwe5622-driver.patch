+	init_completion(&work_data->callback_complete);
+	INIT_LIST_HEAD(&work_data->rx_tx_head);
+	if (work_data->wcn_usb_thread)
+		wake_up_process(work_data->wcn_usb_thread);
+	else
+		wcn_usb_err("%s create a new thread failed\n", __func__);
+}
