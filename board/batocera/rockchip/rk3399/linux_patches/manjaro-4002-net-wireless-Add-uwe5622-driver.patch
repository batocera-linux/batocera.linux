+	channel = chnmg_find_channel(chnmg, channel_id);
+	if (!channel) {
+		channel = channel_init(channel_id, chnmg->dir, pop_link);
+		for (i = 0; i < WCN_USB_CHANNEL_MAX; i++) {
+			if (!chnmg->channel[i]) {
