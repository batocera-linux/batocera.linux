+static void get_cmd_par(char *str, struct nvm_cali_cmd *cmd)
+{
+	int i, j, bufType, cType, flag;
+	char tmp[128];
+	char c;
+	s64 val;
+
+	bufType = -1;
+	cType = 0;
+	flag = 0;
+	memset(cmd, 0, sizeof(struct nvm_cali_cmd));
+	for (i = 0, j = 0;; i++) {
+		c = str[i];
+		cType = find_type(c);
+		if ((cType == 1) || (cType == 2) ||
+			(cType == 3)) {
+			tmp[j] = c;
+			j++;
+			if (bufType == -1) {
+				if (cType == 2)
+					bufType = 2;
+				else
+					bufType = 1;
+			} else if (bufType == 2) {
+				if (cType == 1)
+					bufType = 1;
+			}
+			continue;
+		}
+		if (-1 != bufType) {
+			tmp[j] = '\0';
+
+			if ((bufType == 1) && (flag == 0)) {
+				strcpy(cmd->itm, tmp);
+				flag = 1;
+			} else {
+				if (kstrtos64(tmp, 0, &val))
+					pr_err("kstrtos64 %s: error is %d\n",
+					       tmp, kstrtos64(tmp, 0, &val));
+				cmd->par[cmd->num] = val & 0xFFFFFFFFFFFFFFFF;
+				cmd->num++;
+			}
+			bufType = -1;
+			j = 0;
+		}
+		if (cType == 0)
+			continue;
+		if (cType == 4)
