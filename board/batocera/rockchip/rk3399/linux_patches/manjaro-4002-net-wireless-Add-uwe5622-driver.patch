+void wcn_power_domain_set(struct wcn_device *wcn_dev, u32 set_type)
+{
+	u32 offset0 = 0, offset1 = 0;
+	u32 bitmap0 = 0, bitmap1 = 0;
+
+	if (wcn_platform_chip_type() == WCN_PLATFORM_TYPE_PIKE2) {
+		if (set_type == 1) {
+			offset0 = 0X2050;
+			offset1 = 0X1050;
+			bitmap0 = 1 << 24;
+			bitmap1 = 1 << 25;
+		} else {
+			offset0 = 0X2050;
+			offset1 = 0X2050;
+			bitmap0 = 1 << 24;
+			bitmap1 = 1 << 25;
+		}
+	} else {
+		if (set_type == 1) {
+			offset0 = 0X2100;
+			offset1 = 0X1100;
+			bitmap0 = 1 << 24;
+			bitmap1 = 1 << 25;
+		} else {
+			offset0 = 0X2100;
+			offset1 = 0X2100;
+			bitmap0 = 1 << 24;
+			bitmap1 = 1 << 25;
+		}
+	}
+	wcn_regmap_raw_write_bit(wcn_dev->rmap[REGMAP_PMU_APB],
+					 offset0, bitmap0);
+	wcn_regmap_raw_write_bit(wcn_dev->rmap[REGMAP_PMU_APB],
+					 offset1, bitmap1);
+}
