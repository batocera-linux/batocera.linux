+					u8 type,
+					u8 offset)
+{
+	struct tx_msdu_dscr *dscr;
+	struct sprdwl_intf *dev;
+	u8 lut_index;
+	struct sk_buff *temp_skb;
+#define DSCR_LEN	11
+#define MSG_PTR_LEN 8
+
+	if (skb_headroom(skb) < (DSCR_LEN + intf->hif_offset + MSG_PTR_LEN)) {
+		temp_skb = skb;
