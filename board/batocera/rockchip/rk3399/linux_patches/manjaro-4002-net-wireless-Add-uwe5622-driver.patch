+	if (ret) {
+		mbuf_list_iter(head, num, mbuf, i)
+		sprdwcn_bus_list_free(channel_id, head, tail, num);
+	} else {
+		channel->lp_tx_head += num;
+	}
+
+	mutex_unlock(&channel->pool_lock);
+	return ret;
