+	}
+	/*alloc five byte for fw 16 byte need
+	 *dscr:11+flag:5 =16
+	 */
+	skb_push(skb, FLAG_SIZE);
+	memcpy(skb->data, temp_flag, FLAG_SIZE),
+	/*send group in BK to avoid FW hang*/
+	dscr = (struct tx_msdu_dscr *)skb->data;
+	if ((vif->mode == SPRDWL_MODE_AP ||
