+#ifndef __MCHN_H__
+#define __MCHN_H__
+
+#include <linux/kernel.h>
+#include <linux/slab.h>
+#include <linux/spinlock.h>
+#include <linux/interrupt.h>
+
+#define HW_TYPE_SDIO 0
+#define HW_TYPE_PCIE 1
+#define MCHN_MAX_NUM 32
+
+struct buffer_pool {
+	int size;
+	int free;
+	int payload;
+	void *head;
+	char *mem;
