+void sdiohal_resume_check(void)
+{
+	struct sdiohal_data_t *p_data = sdiohal_get_data();
+	unsigned int cnt = 0;
+
+	while ((!atomic_read(&p_data->flag_resume)) && (cnt++ < 5000)) {
+		if (cnt == 0) {
+			sdiohal_err("wait sdio resume %s\n", __func__);
+			dump_stack();
+		}
+		usleep_range(4000, 6000);
+	}
+}
