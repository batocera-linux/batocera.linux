+
+static int wcn_usb_channel_open(struct inode *inode, struct file *file)
+{
+	struct channel *channel;
+
+#if (LINUX_VERSION_CODE >= KERNEL_VERSION(5,19, 2))
+	channel = (struct channel *)pde_data(inode);
+else
