+int mchn_push_link_wait_complete(int chn, struct mbuf_t *head,
+				 struct mbuf_t *tail, int num, int timeout);
+int mchn_hw_pop_link(int chn, void *head, void *tail, int num);
+int mchn_hw_tx_complete(int chn, int timeout);
