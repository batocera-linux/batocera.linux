+/dts-v1/;
+
+#include "meson-sm1-x96-max-plus.dts"
+
+/ {
+	model = "AMedia X96 Max+ (eth0 speed:100Mb/s)";
+};
+
+&external_phy  {
+	max-speed = <100>;
+};
