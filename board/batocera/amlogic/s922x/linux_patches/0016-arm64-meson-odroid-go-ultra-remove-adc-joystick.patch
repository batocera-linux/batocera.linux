@@ -21,52 +21,6 @@ aliases {
 		rtc0 = &vrtc;
 	};
 
-	adc-joystick-left {
-		compatible = "adc-joystick";
-		io-channels = <&saradc 2>, <&saradc 3>;
-		poll-interval = <10>;
