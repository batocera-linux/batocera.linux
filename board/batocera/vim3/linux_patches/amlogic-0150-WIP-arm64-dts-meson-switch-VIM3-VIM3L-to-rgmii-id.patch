+++ b/arch/arm64/boot/dts/amlogic/meson-khadas-vim3.dtsi
@@ -216,9 +216,8 @@
         pinctrl-0 = <&eth_pins>, <&eth_rgmii_pins>;
         pinctrl-names = "default";
         status = "okay";
-        phy-mode = "rgmii";
+        phy-mode = "rgmii-id";
         phy-handle = <&external_phy>;
-        amlogic,tx-delay-ns = <2>;
 };
 
 &hdmi_tx {
